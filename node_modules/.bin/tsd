#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/srv/misc/node-mongodb-native/node_modules/.pnpm/tsd@0.31.2/node_modules/tsd/dist/node_modules:/srv/misc/node-mongodb-native/node_modules/.pnpm/tsd@0.31.2/node_modules/tsd/node_modules:/srv/misc/node-mongodb-native/node_modules/.pnpm/tsd@0.31.2/node_modules:/srv/misc/node-mongodb-native/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/srv/misc/node-mongodb-native/node_modules/.pnpm/tsd@0.31.2/node_modules/tsd/dist/node_modules:/srv/misc/node-mongodb-native/node_modules/.pnpm/tsd@0.31.2/node_modules/tsd/node_modules:/srv/misc/node-mongodb-native/node_modules/.pnpm/tsd@0.31.2/node_modules:/srv/misc/node-mongodb-native/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsd/dist/cli.js" "$@"
else
  exec node  "$basedir/../tsd/dist/cli.js" "$@"
fi
